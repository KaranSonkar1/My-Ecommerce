{
  "name": "ecommerce-mern",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel start:backend start:frontend",
    "start:backend": "cd ecommerce-backend && npm run dev",
    "start:frontend": "cd ecommerce-frontend && npm run dev",
    "install:all": "npm install && cd ecommerce-backend && npm install && cd ../ecommerce-frontend && npm install"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
